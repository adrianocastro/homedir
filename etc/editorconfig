# editorconfig.org

# top-most EditorConfig file
root = true

[*]
# all files
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# css files
[*.css]
indent_style = space
indent_size = 4

# html files
[*.html]
indent_style = space
indent_size = 4

# js files
[*.js]
indent_style = space
indent_size = 4

